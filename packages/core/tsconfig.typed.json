{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "declaration": true,
    "noEmit": false
  },
  "include": [
    // 指定需要编译的文件
    "typings",
    "src",
  ],
  "exclude": [
    // 指定不需要编译的文件
    "node_modules",
    "dist/**/*",
    "**/*.spec.ts",
    "jest/**/*",
    "**/__tests__/**/*",
    "test"
  ]
}
