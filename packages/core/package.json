{
  "name": "@acme-ui/core",
  "version": "0.0.0",
  "description": "Next Gen UI Design Language and Library",
  "author": "Yufu FE Team",
  "homepage": "https://github.com/yufuid/acme-ui#readme",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "unpkg": "dist/umd/acme-ui.production.min.js",
  "typings": "dist/cjs/index.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yufuid/acme-ui.git"
  },
  "scripts": {
    "build": "yarn lint-staged:style src/ && yarn build:esm && yarn build:cjs && yarn build:umd",
    "build:esm": "node scripts/build.js esm",
    "build:cjs": "node scripts/build.js cjs",
    "build:umd": "node scripts/build.js umd",
    "build:typed": "tsc --emitDeclarationOnly -p ./tsconfig.typed.json",
    "build:style": "gulp",
    "lint-staged": "lint-staged",
    "check:tsc": "tsc",
    "lint-staged:js": "cross-env-shell ESLINT_ENV=commit eslint --fix --ext .js,.jsx",
    "lint-staged:ts": "cross-env-shell ESLINT_ENV=commit eslint --fix --ext .ts,.tsx",
    "lint-staged:style": "stylelint --fix --syntax less",
    "test": "node scripts/test.js -o",
    "test:all": "node scripts/test.js --all --passWithNoTests",
    "test:watch": "node scripts/test.js --watch -o",
    "test:files": "node scripts/test.js --findRelatedTests",
    "test:coverage": "node scripts/test.js --coverage"
  },
  "bugs": {
    "url": "https://github.com/yufuid/acme-ui/issues"
  },
  "sideEffects": [
    "dist/umd/*",
    "dist/es/**/*.css",
    "dist/cjs/**/*.css",
    "*.less"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/plugin-proposal-export-default-from": "^7.12.1",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-function-sent": "^7.12.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-classes": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "autoprefixer": "^10.2.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^9.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "jest-watch-typeahead": "^0.6.1",
    "less": "3.13.1",
    "lint-staged": "^10.5.3",
    "postcss": "^8.2.4",
    "postcss-less": "^4.0.0",
    "postcss-url": "^10.1.1",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.37.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "stylelint": "^13.11.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "typescript": "4.1.5"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  },
  "peerDependencies": {
    "@types/react": ">=16.8.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
